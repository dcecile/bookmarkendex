{
  "name": "Bookmarkendex",
  "version": "1.0",
  "manifest_version": 2,

  "description": "A search extension that provides quick access to bookmarks.",
  "icons": {
    "16": "images/icon_16.png",
    "24": "images/icon_24.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "64": "images/icon_64.png",
    "128": "images/icon_128_padded.png",
    "256": "images/icon_256_padded.png"
  },

  "omnibox": {
    "keyword": "b"
  },

  "background": {
    "scripts": [
      "test.js",
      "bookmark.js",
      "test/bookmark.js",
      "search.js",
      "test/search.js",
      "result.js",
      "test/result.js",
      "event.js"
    ],
    "persistent": false
  },

  "homepage_url": "https://github.com/dcecile/bookmarkendex",

  "incognito": "split",
  "permissions": [
    "bookmarks"
  ]
}
